// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://postgres:postgres@127.0.0.1:5432/users"
}


model User{
  id Int @id @default(autoincrement())
  username String 
  email String @unique
  password String
  isActive Boolean
  isSuspended Boolean
}
